{
  "name": "Hospital Healthcare - Light Clean Theme",

  "dataColors": [
    "#007ACC",
    "#4DB6AC",
    "#66BB6A",
    "#43A047",
    "#FBC02D",
    "#E53935",
    "#8E24AA",
    "#5C6BC0"
  ],

  "background": "#FFFFFF",
  "foreground": "#2E2E2E",
  "tableAccent": "#007ACC",

  "textClasses": {
    "title": { "fontSize": 20, "fontFace": "Segoe UI Semibold" },
    "header": { "fontSize": 12, "fontFace": "Segoe UI Semibold" },
    "label": { "fontSize": 11, "fontFace": "Segoe UI" },
    "callout": { "fontSize": 28, "fontFace": "Segoe UI Semibold" }
  },

  "visualStyles": {
    "*": {
      "*": {
        "border": [{ "show": false }],
        "shadow": [{ "show": false }],
        "visualHeader": [{ "show": false }]
      }
    },

    "page": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#F7F9FB" } }
          }
        ]
      }
    },

    "card": {
      "*": {
        "cardStyle": [{ "style": "outline" }],
        "background": [
          {
            "color": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "border": [
          { 
            "show": true,
            "color": { "solid": { "color": "#E0E0E0" } },
            "radius": 6
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#007ACC" } },
            "fontSize": 18,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "categoryLabels": [
          {
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "color": { "solid": { "color": "#6A6A6A" } }
          }
        ]
      }
    },

    "slicer": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "titleText": [
          {
            "color": { "solid": { "color": "#007ACC" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "items": [
          {
            "color": { "solid": { "color": "#333333" } },
            "fontSize": 11,
            "fontFamily": "Segoe UI"
          }
        ]
      }
    },

    "tableEx": {
      "*": {
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#FFFFFF" } },
            "backColor": { "solid": { "color": "#007ACC" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "values": [
          {
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "backColor": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "grid": [
          {
            "vertGrid": { "color": { "solid": { "color": "#D9D9D9" } } },
            "horizGrid": { "color": { "solid": { "color": "#D9D9D9" } } }
          }
        ]
      }
    },

    "matrix": {
      "*": {
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#FFFFFF" } },
            "backColor": { "solid": { "color": "#007ACC" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI Semibold"
          }
        ],
        "rowHeaders": [
          {
            "fontColor": { "solid": { "color": "#333333" } },
            "fontSize": 11,
            "fontFamily": "Segoe UI"
          }
        ]
      }
    },

    "clusteredColumnChart": {
      "*": {
        "title": [
          {
            "color": { "solid": { "color": "#007ACC" } },
            "fontSize": 14,
            "fontFamily": "Segoe UI Semibold"
          }
        ]
      }
    },

    "lineChart": {
      "*": {
        "title": [
          {
            "color": { "solid": { "color": "#007ACC" } },
            "fontSize": 14,
            "fontFamily": "Segoe UI Semibold"
          }
        ]
      }
    }
  }
}
